    "url": "https://github.com/OmarKhled/IEEE-NU/issues"
  },
  "homepage": "https://github.com/OmarKhled/IEEE-NU#readme",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "colors": "^1.4.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "express-fileupload": "^1.2.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.13.5",
    "platform": "^1.3.6",
    "react-meta-tags": "^1.0.1"
  },
  "devDependencies": {
    "concurrently": "^6.2.0",
    "nodemon": "^2.0.12"
